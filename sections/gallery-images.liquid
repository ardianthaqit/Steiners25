      },
      {
        "type": "image_picker",
        "id": "thirdimage",
        "label": "Third Image"
      },
      {
        "type": "image_picker",
        "id": "fourthimage",
        "label": "Fourth Image"
      },
      {
        "type": "image_picker",
        "id": "fiveimage",
        "label": "Five Image"
      }


  ],
  "presets": [{
